{
    "name": "proshore/entrust-lumen",
    "description": "This package provides a flexible way to add Role-based Permissions to Lumen",
    "keywords": ["lumen","illuminate","auth","roles","acl","permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "Proshore",
            "email": "babishshrestha@gmail.com"
        },
        {
            "name": "Saroj",
            "email": "saroj@proshore.eu"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/console": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/cache": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "illuminate/database": "^6.0",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "psr-4": {
            "Zizaco\\Entrust\\": "src/Entrust/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Middleware/MiddlewareTest.php"
        ]
    },
    "minimum-stability": "dev"
}
